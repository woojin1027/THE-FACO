var scrollTop = {
    init: function() {
        this.scrollT();
    },
    scrollT: function() {
        $('.rnb .rnb-12').on('click', function(event) {
            event.preventDefault();
            $('body,html').animate({
                scrollTop: 0
            }, 700);
        });
    }
};
scrollTop.init();





