var tabFun = {
    init: function() {
        this.showHide();
    },
    showHide: function() {
        $tabmeno = $("div.sub-contert-box div.panel");
        $("#tap01").show();

        $tab = $("ul.panelul li a");
        $tab.on('click', function(event) {
            event.preventDefault();
            $tab.removeClass("selected03");
            $(this).addClass("selected03");
            $tabmeno.hide();
            $($(this).attr("href")).show();
        });
    }

};
tabFun.init();



