/**
 * ?��?��?��?��?��?�� ?��?��?��?��?��?��?��?��?��?��
 * @type {mainSlider}
 */
var mainSlider = {
    init: function() {
        this.slide();
    },
    slide: function() {
        var join_wrap = $('#join-wrap'),
            info_wrap = $('#info-wrap'),
            join = $('#join'),
            info = $('#info'),
            btn = $('.slider-btn'),
            lnb_04 = $('.lnb-04'),
            lnb = $('#lnb'),
            rnb = $('#rnb'),
            lnb_02 = $('#lnb-02');

        // bxslider ?��?��?��?��?��?��처占?��?��
        info.hide();
        info_wrap.hide();
        // join.hide();
        // join_wrap.hide();

        btn.add(lnb_04).on('click', function(event) {
            event.preventDefault();
            
            if (info.css('display') === 'none') {
                // 1. ?��?��?��?��?��?��?��?��?��?�� ?��?��?��?��?��?��.info block
                join.hide(300);
                join_wrap.hide(300);

                info.show(300);
                info_wrap.show(300);

                // 2. ?��?��?��?��?��?��?��?���? ?��?��?���? ?��?��?��?��?��?��
                // lnb.css({'right':'0','left':'auto'});
                lnb.hide();
                lnb_02.show();
                rnb.css({'left':'0','right':'auto'});

                // 3. btn ?��?��?��치占?��?��?��?��?��
                btn.attr('class','slider-btn right');

            } else {
                // 1. ?��?��?��?��?��?��?��?��?��?�� ?��?��?��?��?��?��.info none
                info.hide(300);
                info_wrap.hide(300);
                join.show(300);
                join_wrap.show(300);


                // 2. ?��?��?��?��?��?��?��?���? ?��?��?���? ?��?��?��?��?��?��
                // lnb.css({'left':'0','right':'auto'});
                lnb_02.hide();
                lnb.show();
                rnb.css({'right':'0','left':'auto'});

                // 3. btn ?��?��?��치占?��?��?��?��?��
                btn.attr('class','slider-btn left');
            }
            
        });
    }
};
mainSlider.init();




