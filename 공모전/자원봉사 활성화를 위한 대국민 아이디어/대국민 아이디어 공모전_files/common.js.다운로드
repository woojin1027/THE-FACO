 var familyFunction = {
     init: function() {
         this.family_01();
         this.family_02();
         this.family_03();    //?��?��?��?��?��?�� ?��?���?몌옙 ?��?��?��?��?��?��?�� 
         this.family_04();    //?��?��?��?��?��?�� ?��?���?몌옙 ?��?��?��?��?��?��?�� 
     },
     family_01: function() {
         var dt = $('.footer-right .select-box-01 dt a'),
           dd = $('.footer-right .select-box-01 dd');

         dt.on('click', function() {
           if ( dd.css('display') == 'none' ) {
             dd.slideDown(200);
           } else {
             dd.slideUp(200);
           }
         });
     },
     family_02: function() {
         var dt = $('.footer-right .select-box-02 dt a'),
           dd = $('.footer-right .select-box-02 dd');

         dt.on('click', function() {
           if ( dd.css('display') == 'none' ) {
             dd.slideDown(200);
           } else {
             dd.slideUp(200);
           }
         });
     },
     family_03: function() {
         var dt = $('.footer-right-sub .select-box-01 dt a'),
           dd = $('.footer-right-sub .select-box-01 dd');

         dt.on('click', function() {
           if ( dd.css('display') == 'none' ) {
             dd.slideDown(200);
           } else {
             dd.slideUp(200);
           }
         });
     },
     family_04: function() {
         var dt = $('.footer-right-sub .select-box-02 dt a'),
           dd = $('.footer-right-sub .select-box-02 dd');

         dt.on('click', function() {
           if ( dd.css('display') == 'none' ) {
             dd.slideDown(200);
           } else {
             dd.slideUp(200);
           }

         });
     }
 };
 familyFunction.init();






