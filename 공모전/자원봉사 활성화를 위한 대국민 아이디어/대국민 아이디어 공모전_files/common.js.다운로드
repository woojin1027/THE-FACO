function sbx_removeNotMe(selbox){
	if(selbox){
		if(selbox.options){
			var p_selbox_len = selbox.options.length;
			var p_cur_val = selbox.value;
			for(var i=(p_selbox_len-1);i>=0;i--){
				if(p_cur_val!=selbox.options[i].value)
				selbox.options[i].outerHTML = "";
			}
		}
	}
}

