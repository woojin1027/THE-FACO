    init: function() {
        this.lnbAddClass();
    },
    lnbAddClass: function() {
        var last = $('.sub-lnb .sub-lnb-inner .lnb-list li').last().children('a');
        last.addClass('last');
        // console.log(last.text());
    }
};
lnbLast.init();



























































































