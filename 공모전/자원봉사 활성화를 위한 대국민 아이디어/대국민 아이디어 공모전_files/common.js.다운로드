    },
    slider_01: function() {
         //?��?��?��?��?��?�� 첫占?��?���? ?��?��?��?��?��?��?��?��?�� ?��?��?��?��?��?��?��?��?��?��
         var mySwiper = new Swiper('#slider-01 .swiper-container',{
          pagination: '#slider-01 .pagination',
          loop:true,
          grabCursor: true,
          autoplay: 3000,
          paginationClickable: true
        });
         
        
    },
